# FHIR Ingestor Configuration
# Copy this to .env and update with your settings

# MongoDB Connection
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority

# Database name
DB_NAME=fhir_db

# Input directory for JSON files (optional, for file mode)
INPUT_DIR=./fhir_data

# HTTP server port (optional, for HTTP mode)
SERVER_PORT=5000